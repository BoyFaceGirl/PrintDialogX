<Window x:Class="PrintDialogX.PrintControl.PrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print"
        FontSize="12"
        MinWidth="750"
        MinHeight="450"
        Background="#FFF0F0F0"
        Icon="Resources/WindowIcon.ico">

    <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden">
        <Frame.Content>
            <Grid Grid.ColumnSpan="2" Background="#FFF0F0F0">
                <Image x:Name="iconImage" MaxWidth="256" MaxHeight="256" Stretch="UniformToFill" Source="Resources/WindowIcons/Icon 256x256.png"></Image>
            </Grid>
        </Frame.Content>
    </Frame>
</Window>

